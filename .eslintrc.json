{
	"extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
	"globals": {
		"_": true,
		"$": true,
		"Vue": true,
		"VueRouter": true,
		"Snap": true
	},
	"rules": {
		"eqeqeq": 2,
		"no-unused-vars": [2, {"vars": "local"}],
		"no-redeclare": 2,
		"no-native-reassign": 2,
		"no-extend-native": 2,
		"no-eval": 2,
		"no-alert": 2,
		"semi": 1,
		"curly": 1,
		"brace-style": 1,
		"no-warning-comments": 1,
		"quotes": [1, "single", "avoid-escape"],
		"space-infix-ops": 1,
		"space-before-blocks": 1,
		"keyword-spacing": 1,
		"no-multi-spaces": 1,
		"no-trailing-spaces": 1,
		"no-multiple-empty-lines": 1,
		"key-spacing": 1,
		"semi-spacing": 1,
		"comma-spacing": 1
	}
}
